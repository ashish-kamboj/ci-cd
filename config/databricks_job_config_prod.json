{
  "name": "ML_Regression_Training_Prod",
  "notebook_task": {
    "notebook_path": "/ml-regression-model-prod/notebooks/train",
    "base_parameters": {}
  },
  "new_cluster": {
    "spark_version": "13.3.x-scala2.12",
    "node_type_id": "Standard_DS3_v2",
    "num_workers": 0,
    "spark_conf": {
      "spark.databricks.cluster.profile": "singleNode",
      "spark.master": "local[*]"
    },
    "custom_tags": {
      "ResourceClass": "SingleNode"
    }
  },
  "timeout_seconds": 3600,
  "max_retries": 2,
  "schedule": {
    "quartz_cron_expression": "0 0 2 * * ?",
    "timezone_id": "UTC",
    "pause_status": "PAUSED"
  },
  "email_notifications": {
    "on_success": [],
    "on_failure": [],
    "no_alert_for_skipped_runs": false
  },
  "max_concurrent_runs": 1
}
